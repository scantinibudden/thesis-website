{
    "version": 2,
    "builds": [
      {
        "src": "api/api.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/api.py",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "https://thesis-experiment.vercel.app"
        }
      }
    ]
  }
  